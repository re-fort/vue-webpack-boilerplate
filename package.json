{
  "name": "vue-webpack-boilerplate",
  "description": "A Webpack boilerplate with vue-loader,vue-resource and vue-router",
  "author": "re-fort",
  "license": "MIT",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/re-fort/vue-webpack-boilerplate.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "babel-runtime": "6.18.0",
    "bulma": "0.2.3",
    "font-awesome": "4.7.0",
    "vue": "2.0.3",
    "vue-resource": "1.0.3",
    "vue-router": "2.0.1"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "json-loader": "0.5.4",
    "node-sass": "3.11.0",
    "pug": "^2.0.0-beta6",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "vue-hot-reload-api": "2.0.6",
    "vue-html-loader": "1.2.3",
    "vue-loader": "9.7.0",
    "vue-style-loader": "1.0.0",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2"
  }
}
