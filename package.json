{
  "name": "vue-webpack-boilerplate",
  "description": "A webpack boilerplate with vue-loader,axios, vue-router and vuex",
  "author": "re-fort",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/re-fort/vue-webpack-boilerplate.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "pre-build": "npm run clean && webpack --progress --hide-modules",
    "clean": "rimraf ./dist",
    "lint": "eslint src tests/**/*",
    "test": "npm run lint && cross-env NODE_ENV=testing npm run test:unit",
    "test:unit": "karma start tests/unit/karma.conf.js --single-run"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "bulma": "^0.6.1",
    "font-awesome": "^4.7.0",
    "js-cookie": "^2.2.0",
    "vue": "^2.5.8",
    "vue-analytics": "^5.4.0",
    "vue-router": "^3.0.1",
    "vue-spinner": "^1.0.3",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.4.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.0",
    "eslint-plugin-vue": "beta",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.6",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "power-assert": "^1.4.4",
    "pug": "^2.0.0-rc.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.8",
    "vue-test-utils": "vue-test-utils@1.0.0-beta.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-espower-loader": "^1.0.2"
  }
}
